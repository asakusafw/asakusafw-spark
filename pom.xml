  <description>Asakusa DSL Compiler for Spark Project Root</description>
  <url>https://asakusafw.com</url>
  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
